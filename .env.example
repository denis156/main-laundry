# ==============================================================================
# APPLICATION CONFIGURATION
# ==============================================================================
# Core application settings including name, environment, and debug mode

APP_NAME="Main Laundry"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# ==============================================================================
# LOCALIZATION SETTINGS
# ==============================================================================
# Configure language, locale, and timezone for the application

APP_LOCALE=id
APP_FALLBACK_LOCALE=id
APP_FAKER_LOCALE=id_ID
APP_TIMEZONE=Asia/Makassar

# ==============================================================================
# APPLICATION VERSION
# ==============================================================================
# Version numbers for different application interfaces

ADMIN_APP_VERSION=1.0.0
KURIR_APP_VERSION=1.0.0

# ==============================================================================
# MAINTENANCE MODE
# ==============================================================================
# Configure how maintenance mode is handled

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# ==============================================================================
# SERVER CONFIGURATION
# ==============================================================================

PHP_CLI_SERVER_WORKERS=4
BCRYPT_ROUNDS=12

# ==============================================================================
# LOGGING CONFIGURATION
# ==============================================================================
# Configure how application logs are stored and managed

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# ==============================================================================
# DATABASE CONFIGURATION
# ==============================================================================
# Database connection settings - Configure based on your database type

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=main_laundry
DB_USERNAME=root
DB_PASSWORD=

# ==============================================================================
# SESSION CONFIGURATION
# ==============================================================================
# Configure session driver and settings

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# ==============================================================================
# BROADCAST CONFIGURATION
# ==============================================================================
# Configure broadcasting system for real-time events

BROADCAST_CONNECTION=reverb

# ==============================================================================
# CACHE & QUEUE CONFIGURATION
# ==============================================================================
# Configure caching and queue systems

FILESYSTEM_DISK=public
QUEUE_CONNECTION=sync
CACHE_STORE=database
# CACHE_PREFIX=

# ==============================================================================
# REDIS CONFIGURATION
# ==============================================================================
# Redis cache and queue driver settings

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# ==============================================================================
# MEMCACHED CONFIGURATION
# ==============================================================================

MEMCACHED_HOST=127.0.0.1

# ==============================================================================
# MAIL CONFIGURATION
# ==============================================================================
# Email service configuration

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# ==============================================================================
# AWS S3 CONFIGURATION
# ==============================================================================
# Amazon Web Services S3 storage settings (optional)

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# ==============================================================================
# LARAVEL OCTANE CONFIGURATION
# ==============================================================================
# High-performance application server configuration

OCTANE_SERVER=frankenphp
OCTANE_HTTPS=false

# ==============================================================================
# LARAVEL REVERB CONFIGURATION
# ==============================================================================
# WebSocket server for real-time broadcasting
# Generate new keys using: php artisan reverb:key

REVERB_APP_ID=
REVERB_APP_KEY=
REVERB_APP_SECRET=
REVERB_HOST=localhost
REVERB_PORT=8080
REVERB_SCHEME=http

VITE_REVERB_APP_KEY="${REVERB_APP_KEY}"
VITE_REVERB_HOST="${REVERB_HOST}"
VITE_REVERB_PORT="${REVERB_PORT}"
VITE_REVERB_SCHEME="${REVERB_SCHEME}"

# ==============================================================================
# VITE CONFIGURATION
# ==============================================================================
# Frontend build tool configuration

VITE_APP_NAME="${APP_NAME}"

# ==============================================================================
# SOCIAL MEDIA CONFIGURATION
# ==============================================================================
# Social media and contact information

SOSMED_PHONE=
SOSMED_EMAIL=
SOSMED_INSTAGRAM=
SOSMED_GITHUB=

# ==============================================================================
# WEB PUSH NOTIFICATION CONFIGURATION (VAPID)
# ==============================================================================
# VAPID (Voluntary Application Server Identification) keys for Web Push API
# Used for sending push notifications to browsers
# Generate new keys using: php artisan webpush:vapid

VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
